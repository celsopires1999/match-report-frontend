    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.5",
    "@prisma/client": "^5.0.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "class-validator": "^0.14.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "lodash": "^4.17.21",
    "next": "13.4.7",
    "notistack": "^3.0.1",
