{
  "name": "match-report-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run push:test && dotenv -e .env.test jest -- --runInBand",
    "test:cov": "npm run push:test && dotenv -e .env.test jest -- --runInBand --coverage",
    "studio:test": "dotenv -e .env.test -- npx prisma studio",
    "reset:test": "dotenv -e .env.test -- npx prisma db push --force-reset",
    "reset:dev": "dotenv -e .env.development -- npx prisma db push --force-reset",
    "studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "push:dev": "dotenv -e .env.development -- npx prisma db push",
    "push:test": "dotenv -e .env.test -- npx prisma db push"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
